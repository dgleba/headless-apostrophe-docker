{
  "name": "site",
  "version": "1.0.0",
  "description": "Site",
  "main": "app.js",
  "scripts": {
    "clean": "rm -Rf data && rm -Rf public",
    "dev": "npm run dev:app & npm run dev:webpack",
    "dev:app": "node-dev app.js",
    "dev:webpack": "node app custom:webpack-dev-server",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "prettier-standard 'lib/**/*.scss' && stylelint 'lib/**/*.scss' --fix",
    "lint:js": "prettier-standard 'lib/**/*.js' 'app.js'",
    "precommit": "lint-staged",
    "prestart": "npm run webpack",
    "start": "node app",
    "webpack": "node app custom:webpack"
  },
  "lint-staged": {
    "lib/**/*.js": [
      "lint:js",
      "git add"
    ],
    "app.js": [
      "lint:js",
      "git add"
    ],
    "lib/**/*.scss": [
      "lint:css",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": "8"
  },
  "author": "Anthony Tarlao @falkodev",
  "license": "MIT",
