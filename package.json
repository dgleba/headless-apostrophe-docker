    "chai": "^4.0.2",
    "chai-spies": "^0.7.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "hard-source-webpack-plugin": "^0.4.3",
